 
public class problem1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//String 배열 sArr을 사과 딸기 오렌지로 초기화 시키시오
		String[] sArr = {"사과","딸기","오렌지"};
		//2. 구구단 2단을 정상적으로 출력하기 위하여 다음 빈칸에 들어갈 알맞은 for문을 작성하시오
		for(int i = 1; i<10;i+=2)
		{
			System.out.println("2 * " + i + " = " + 2*i);
		}
		for(int i = 2; i<10;i+=2)
		{
			System.out.println("2 * " + i + " = " + 2*i);
		}
		//3. 2단을 정상적으로 출력하기 위하여 다음 빈칸에 들어갈 알맞은 for문을 작성하시오
		//4. 배열 깊은 복사를 할 수 있는 방법 세가지를 고르시오
		//1. String 내부의 clone()함수를 이용한다.
		//Arrays.copy
		//Arrays.copyof
		double[] dArr = new double[5];
		//아래 배열을 for문을 이용하여 콘솔창에 출력하는 코드를 작성하시오
		int[] iArr = {1,2,3,4,5};
		for(int i : iArr) {
			System.out.println(i);
		}
		}
	
	

}
