package A_list.listSort;

import java.io.Serializable;

public class Customer implements Serializable{
	private String m_name;
	private int m_age;
	private char m_gender;
	private double m_point;

	Customer(String name, int age, char gender, double point){
	 m_name= name;
	 m_age = age;
	 m_gender = gender;
	 m_point = point;
	}
	public String getM_name() {
		return m_name;
	}
	public void setM_name(String m_name) {
		this.m_name = m_name;
	}
	public int getM_age() {
		return m_age;
	}
	public void setM_age(int m_age) {
		this.m_age = m_age;
	}
	public char getM_gender() {
		return m_gender;
	}
	public void setM_gender(char m_gender) {
		this.m_gender = m_gender;
	}
	public double getM_point() {
		return m_point;
	}
	public void setM_point(double m_point) {
		this.m_point = m_point;
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
	}

}
